#include <stdio.h>
#include <string.h>
#include "Token.h"

using namespace std;

Token::Token()
{
	m_tokType = ::ERROR_TOK;
	m_tokString.clear();

	m_curLen = 0;
	memset(m_tokText, '\0', MAX_TOK_LEN);
}

Token::~Token()
{
}

Token::Token(const Token &t)
: m_tokType(t.m_tokType)
, m_tokString(t.m_tokString)
{
}

Token& Token::operator= (const Token &t)
{
	this->m_tokType = t.m_tokType;
	this->m_tokString = t.m_tokString;


	return *this;
}



